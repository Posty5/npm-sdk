{
    "name": "@posty5/qr-code",
    "version": "1.0.1",
    "description": "QR Code management SDK for Posty5 API",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/Posty5/npm-sdk/tree/main/posty5-qr-code"
    },
    "homepage": "https://github.com/Posty5/npm-sdk#readme",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "posty5",
        "qr-code",
        "qr-generator",
        "sdk",
        "api",
        "typescript"
    ],
    "author": "Posty5",
    "license": "MIT",
    "peerDependencies": {
        "@posty5/core": "^1.0.0"
    },
    "devDependencies": {
        "@posty5/core": "file:../posty5-core",
        "@types/node": "^20.0.0",
        "typescript": "^5.3.0",
        "tsup": "^8.0.0"
    }
}