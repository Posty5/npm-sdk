{
    "name": "@posty5/sdk",
    "version": "1.0.1",
    "description": "Posty5 SDK - TypeScript/JavaScript client libraries for Posty5 API",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Posty5/npm-sdk"
    },
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build:all": "npm run build --workspaces",
        "clean": "npm run clean --workspaces"
    },
    "workspaces": [
        "posty5-core",
        "posty5-short-link",
