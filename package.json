        "posty5-html-hosting-form-submission",
        "posty5-social-publisher-workspace",
        "posty5-social-publisher-task"
    ],
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    },
    "keywords": [
        "posty5",
        "sdk",
        "api-client",
        "typescript",
        "short-link",
        "qr-code",
        "html-hosting",
        "social-publisher"
    ],
    "author": "Posty5",
    "license": "MIT"
